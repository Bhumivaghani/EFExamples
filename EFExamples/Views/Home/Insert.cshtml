@model EFExamples.Models.Student
@{
    ViewBag.Title = "Insert";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="panel-body">
    

    @using (Html.BeginForm("Insert", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {

        <div class="panel-body">
            @Html.HiddenFor(x => x.id)

            <div class="form-group">
                <label>Name</label>
                @Html.TextBoxFor(x => x.name, new { @class = "form-control" })
            </div>
            <div class="form-group">
                <label>Address</label>
                @Html.TextAreaFor(x => x.address, new { @class = "form-control" })
            </div>
            <div class="form-group">
                <label>Age</label>
                @Html.TextBoxFor(x => x.age, new { @class = "form-control" })
            </div>
            <hr />
            <input type="submit" value="Submit" class="btn btn-danger" />
        </div>

    }
</div>
    